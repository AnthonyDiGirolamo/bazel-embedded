def GetDeviceConfig(device_part_no):
    """ GetDeviceConfig returns the device compiler config for a particular part number

    Args:
        device_part_no is the stm32 part number without the corresponding packaging code
        return device_config for use in compiler configuration
    """
    if device_part_no in _CORTEX_M0_DEVICES:
        return _CORTEX_M0_DEVICE_CONFIG
    if device_part_no in _CORTEX_M3_DEVICES:
        return _CORTEX_M3_DEVICE_CONFIG
    if device_part_no in _CORTEX_M4_DEVICES:
        return _CORTEX_M4_DEVICE_CONFIG
    if device_part_no in _CORTEX_M7_DEVICES:
        return _CORTEX_M7_DEVICE_CONFIG
    fail("Could not find part", "device_part_no")

